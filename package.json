{
  "name": "@aptre/template",
  "description": "Template project for Aperture Robotics projects.",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Aperture Robotics LLC.",
    "email": "support@aperture.us",
    "url": "http://aperture.us"
  },
  "contributors": [
    {
      "name": "Christian Stewart",
      "email": "christian@aperture.us",
      "url": "http://github.com/paralin"
    }
  ],
  "repository": {
    "url": "git+ssh://git@github.com/aperturerobotics/template.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "serve": "bun run dev",
    "preview": "vite preview",
    "build": "vite build",
    "check": "bun run typecheck && bun run lint",
    "typecheck": "tsgo --noEmit",
    "deps": "depcheck --ignore-patterns=eslint.config.js,package.json --ignores depcheck,@aptre/common,prettier,rimraf,typescript,@typescript/native-preview,starpc,@go/github.com",
    "clean": "rimraf ./dist/",
    "codegen": "bun run gen",
    "gen": "bun run go:aptre -- generate",
    "gen:force": "bun run go:aptre -- generate --force",
    "format": "bun run format:go && bun run format:js && bun run format:config",
    "format:config": "prettier --write package.json",
    "format:go": "bun run go:aptre -- format",
    "format:js": "prettier --write './!(vendor|dist)/**/(*.ts|*.tsx|*.js|*.html|*.css)'",
    "test": "bun run test:go && bun run check && bun run test:js",
    "test:js": "echo No JS tests.",
    "test:go": "bun run go:aptre -- test",
    "lint": "bun run lint:go && bun run lint:js",
    "lint:go": "bun run go:aptre -- lint",
    "lint:js": "eslint ./",
    "ci": "bun run build && bun run lint:js && bun run lint:go",
    "prepare": "bun run clean && rimraf ./.tools && bun run vendor",
    "vendor": "go mod vendor",
    "go:aptre": "go run -mod=mod github.com/aperturerobotics/common/cmd/aptre",
    "precommit": "bun run format"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@aptre/common": "^0.30.3",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.1.2",
    "@typescript/native-preview": "^7.0.0-dev.20250220",
    "@vitejs/plugin-react": "^5.1.4",
    "depcheck": "^1.4.7",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@aptre/protobuf-es-lite": "^1.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "starpc": "^0.46.2"
  },
  "resolutions": {
    "@aptre/protobuf-es-lite": "^1.0.0"
  }
}
