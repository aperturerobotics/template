{
  "name": "@aptre/template",
  "description": "Template project for Aperture Robotics projects.",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Aperture Robotics LLC.",
    "email": "support@aperture.us",
    "url": "http://aperture.us"
  },
  "contributors": [
    {
      "name": "Christian Stewart",
      "email": "christian@aperture.us",
      "url": "http://github.com/paralin"
    }
  ],
  "repository": {
    "url": "git+ssh://git@github.com/aperturerobotics/template.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "serve": "bun run dev",
    "preview": "vite preview",
    "build": "tsc -b && vite build",
    "check": "bun run typecheck",
    "typecheck": "tsc --noEmit",
    "deps": "depcheck --ignore-patterns=.eslintrc.cjs,package.json --ignores depcheck,rimraf,prettier,typescript,starpc,@go/github.com,@aptre/common",
    "codegen": "bun run gen",
    "ci": "bun run build && npm run lint:js && npm run lint:go",
    "format": "bun run format:go && npm run format:js && npm run format:config",
    "format:config": "prettier --write package.json",
    "format:go": "bun run go:aptre -- format",
    "format:js": "bun run format:js:changed",
    "format:js:changed": "git diff --name-only --diff-filter=d HEAD | grep '\\(\\.ts\\|\\.tsx\\|\\.html\\|\\.css\\|\\.scss\\)$' | xargs -I {} prettier --write {}",
    "format:js:all": "prettier --write './!(vendor|dist)/**/(*.ts|*.tsx|*.js|*.html|*.css)'",
    "gen": "bun run go:aptre -- generate",
    "gen:force": "bun run go:aptre -- generate --force",
    "test": "bun run go:aptre -- test && npm run check && npm run test:js",
    "test:js": "echo No JS tests.",
    "lint": "bun run lint:go && npm run lint:js",
    "lint:go": "bun run go:aptre -- lint",
    "lint:js": "ESLINT_USE_FLAT_CONFIG=false eslint -c .eslintrc.cjs ./example",
    "prepare": "go mod vendor && rimraf ./.tools",
    "go:aptre": "go run -mod=mod github.com/aperturerobotics/common/cmd/aptre",
    "precommit": "bun run format"
  },
  "devDependencies": {
    "@aptre/common": "^0.28.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^5.0.0",
    "depcheck": "^1.4.7",
    "globals": "^17.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@aptre/protobuf-es-lite": "^0.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "starpc": "^0.44.0"
  },
  "resolutions": {
    "@aptre/protobuf-es-lite": "^0.5.0"
  }
}
